{
  "name": "skuflow-backend",
  "version": "1.0.0",
  "description": "SkuFlow - Custom Product Commerce & Production Platform",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "tsx src/db/seed.ts",
    "db:studio": "prisma studio",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "commerce",
    "custom-products",
    "mockups",
    "design",
    "print-shop"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "5.6.0",
    "@sendgrid/mail": "^7.7.0",
    "aws-sdk": "^2.1400.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^7.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "sharp": "^0.32.6",
    "stripe": "^13.5.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "prisma": "5.7.1",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  }
}
